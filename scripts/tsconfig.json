{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "lib": ["es2020"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "dist",
    "baseUrl": "..",
    "rootDir": ".."
  },
  "include": ["./**/*.ts", "../functions/src/**/*.ts"],
  "exclude": ["node_modules"]
} 